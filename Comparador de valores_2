#include <stdio.h>

int main (void){
	int A,B,C;
	
	printf("Indica el valor de A: \n");
    scanf("%d",&A);
    printf("Indica el valor de B: \n");
    scanf("%d",&B);
    printf("Indica el valor de C: \n");
    scanf("%d",&C);
    
    {
		if(A>3)
			printf("El numero es mayor que 3: %d\n", A);
		else
			printf("El numero es menor que 3: %d\n",A);
	}
	{
		if(A>C)
			printf("El numero de A es mayor al de C: %d\n", A,C);
		else
			printf("El numero de A es menor que el de C: %d\n", A,C);
	}
	{
		if(B!=C)
			printf("El valor de B es igual al de C: %d\n", B,C);
		else
			printf("El valor de B no es igual al de C: %d\n", B,C);
	}
	{
		if(A==3)
			printf("El valor de A es igual a 3: %d\n", A);
		else
			printf("El valor de A no es igual a 3: %d\n", A);
	}
	{
		if(A*B==15)
			printf("La multiplicacion es A*B es igual a 15: %d\n",A,B);
		else
			printf("La multiplicacion es A*B no es igual a 15: %d\n",A,B);
	}
	{
		if(C/B<A)
			printf("La division de C entre B es menor que A: %d\n",A,B,C);
		else
			printf("La division de C entre B no es menor que A: %d\n",A,B,C);
	}
	{
		if(C/B==(-10))
			printf("La division de C entre B es igual a -10: %d\n",A,B);
		else
			printf("La division de C entre B no es igual a -10: %d\n",A,B);
	}
	{
		if(A+B+C==5)
			printf("La suma de A+B+C es igual a 5: %d\n",A,B,C);
		else
			printf("La suma de A+B+C no es igual a 5: %d\n",A,B,C);
	}
	{
		if((A+B==8) && (A-B==2))
			printf("Las operaciones dan 8 y 2: %d\n",A,B);
		else
			printf("Las operaciones no dan 8 y 2: %d\n",A,B);
	}
	{
		if((A+B==8) || (A-B==9))
			printf("La condicion se cumple: %d\n",A,B);
		else
			printf("La condicion no se cumple: %d\n",A,B);
	}
	{
		if((A>3) && (B>3) && (C<3))
			printf("La condicion se cumple: %d\n",A,B,C);
		else
			printf("La condicion no se cumple: %d\n",A,B,C);
	}
	return 0;
}
